<h2>Reserved Book Search Results</h2>
<% results = getReservedBookResult %>

<% results.each do |result| %>
    <% result.each do |key, value| %>
      <% if key == "ISNS" %>
          <%= key +": " %>
          <%= value %>
          <br>
          <hr>
      <% elsif key == "cover_url" %>
          <%= image_tag(value) %>   
          <br>
      <% else %>
          <%= key +": " %>
          <%= value %>
          <br>
      <% end %> 
    
    <% end %>
 <% end %>
